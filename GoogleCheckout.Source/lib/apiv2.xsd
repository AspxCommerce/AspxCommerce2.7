  </xs:complexType>
  <xs:complexType name="NotificationDataResponse">
    <xs:all>
      <xs:element name="continue-token" type="xs:string" minOccurs="0" />
      <xs:element name="has-more-notifications" type="xs:boolean" minOccurs="0" />
      <xs:element name="notifications">
        <xs:complexType>
